    <LinearLayout
        android:id="@+id/ll_xgqbm"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:background="@drawable/bg_wallet_item">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/qbsetting_name"
            android:layout_marginLeft="17dp"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="7dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/xgqbm"
                android:textStyle="bold"
                android:textColor="@color/dialog_content"
                android:textSize="15sp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/xgqbm_tip"
                android:textSize="12sp"
                android:textColor="@color/text_title"
                android:layout_marginTop="3dp"/>
        </LinearLayout>
        <View
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/qbsetting_next"
            android:layout_marginRight="17dp"/>
    </LinearLayout>

